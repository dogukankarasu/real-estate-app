<AnchorPane prefHeight="400.0" prefWidth="726.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.user_control">
   <children>
      <Label fx:id="lbl_user" layoutX="193.0" layoutY="17.0" prefHeight="37.0" prefWidth="352.0" text="Label" textFill="#801b1b">
         <font>
            <Font name="Dubai Regular" size="24.0" />
         </font></Label>
      <Button fx:id="bck4_btn" layoutX="14.0" layoutY="20.0" mnemonicParsing="false" onAction="#back2" prefHeight="31.0" prefWidth="140.0" text="Go To Main Menu" />
      <Button fx:id="prfl_btn" layoutX="34.0" layoutY="70.0" mnemonicParsing="false" onAction="#ShowMyInformation" prefHeight="31.0" prefWidth="140.0" text="My Profile" />
