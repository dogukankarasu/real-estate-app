            <TableColumn fx:id="surname" prefWidth="88.0" text="SURNAME" />
            <TableColumn fx:id="username" prefWidth="97.0" text="USERNAME" />
            <TableColumn fx:id="password" prefWidth="92.0" text="PASSWORD" />
            <TableColumn fx:id="email" prefWidth="112.0" text="E-MAIL" />
            <TableColumn fx:id="phone_number" prefWidth="146.0" text="PHONE" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
